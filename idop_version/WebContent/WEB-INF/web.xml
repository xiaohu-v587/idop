<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <filter>
    <filter-name>idop</filter-name>
    <filter-class>com.goodcol.core.core.GcFilter</filter-class>
    <init-param>
      <param-name>configClass</param-name>
      <param-value>com.goodcol.config.OAConfig</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>idop</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>com.goodcol.listener.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>com.goodcol.listener.ParamListener</listener-class>
  </listener>
  <session-config>
    <session-timeout>28</session-timeout>
  </session-config>
</web-app>